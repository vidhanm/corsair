{
  "type": "module",
  "name": "corsair",
  "version": "1.0.0",
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "corsair": "./dist/cli/index.js"
  },
  "scripts": {
    "build-watch": "tsc --watch",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "db:test": "tsx db/index.ts",
    "api:test": "tsx api/index.ts",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "postgrest:reload": "docker kill -s SIGUSR1 corsair-postgrest",
    "postgrest:restart": "docker restart corsair-postgrest",
    "state:test": "tsx state/state.ts",
    "drizzle": "tsx state/drizzle.ts",
    "generate": "./scripts/generate.sh",
    "check": "./scripts/check.sh",
    "migrate": "./scripts/migrate.sh",
    "watch": "tsx cli/watch/index.tsx",
    "test-watch": "tsx cli/watch/test.ts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./core": {
      "types": "./dist/core/index.d.ts",
      "default": "./dist/core/index.js"
    },
    "./client": {
      "types": "./dist/client/index.d.ts",
      "default": "./dist/client/index.js"
    },
    "./db": {
      "types": "./dist/db/index.d.ts",
      "default": "./dist/db/index.js"
    },
    "./db/types": {
      "types": "./dist/db/types.d.ts",
      "default": "./dist/db/types.js"
    },
    "./db/schema-builder": {
      "types": "./dist/db/schema-builder.d.ts",
      "default": "./dist/db/schema-builder.js"
    },
    "./nextjs": {
      "types": "./dist/adapters/nextjs.d.ts",
      "default": "./dist/adapters/nextjs.js"
    },
    "./express": {
      "types": "./dist/adapters/express.d.ts",
      "default": "./dist/adapters/express.js"
    },
    "./schema": {
      "types": "./dist/schema/index.d.ts",
      "default": "./dist/schema/index.js"
    }
  },
  "peerDependencies": {
    "@tanstack/react-query": "^5.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "express": {
      "optional": true
    },
    "fastify": {
      "optional": true
    },
    "hono": {
      "optional": true
    },
    "@cloudflare/workers-types": {
      "optional": true
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "dependencies": {
    "@cerebras/cerebras_cloud_sdk": "^1.59.0",
    "@supabase/postgrest-js": "^2.75.1",
    "@tanstack/react-query": "^5.90.5",
    "@trpc/client": "^11.6.0",
    "@trpc/server": "^11.6.0",
    "@trpc/tanstack-react-query": "^11.6.0",
    "boxen": "^8.0.1",
    "chalk": "^5.6.2",
    "chokidar": "^4.0.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "drizzle-zod": "^0.5.1",
    "ink": "^6.3.1",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "madge": "^8.0.0",
    "openai": "^4.103.0",
    "pg": "^8.16.3",
    "postgrest-js": "2.0.0-rc1",
    "prettier": "^3.3.2",
    "superjson": "^2.2.3",
    "ts-morph": "^27.0.2",
    "tsx": "^4.20.6",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.0.0",
    "@types/express": "^4.17.21",
    "@types/madge": "^5.0.3",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.15.5",
    "@types/prettier": "^3.0.0",
    "@types/react": "^19.2.2",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "bun-types": "latest",
    "drizzle-kit": "^0.22.7",
    "eslint": "^8.57.0",
    "express": "^4.19.2",
    "fastify": "^4.28.0",
    "hono": "^4.4.7",
    "next": "15.5.6",
    "react": "^19.1.0",
    "react-dom": "19.1.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.8"
  }
}
